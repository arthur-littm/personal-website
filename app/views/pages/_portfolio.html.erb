<div class="container text-center" id="portfolio">
  <h1>Portfolio</h1>
  <div class="cards-container">

    <!-- Elephant & Castle -->
    <div class="card">
      <div class="card-image" style="background-image: url(<%= image_path("elephant.png") %>);">
      </div>
      <div class="card-info">
        <p>Travel blog website, with articles, videos and pictures</p>
        <p><strong>My role:</strong> Developer (Freelance)</p>
        <p><strong>Contribution:</strong> 100%</p>
        <p><strong>Technologies:</strong> Ruby on Rails, HTML5, CSS3, WYSIWYG, Cloudinary &amp; ElasticSearch</p>
      </div>
      <div class="card-title">
        <%= image_tag "logo-elephant.png" %>
      </div>
      <div class="card-date">
        <p>2017</p>
      </div>
      <a href="http://www.elephantandcastle.co/" class="card-link"></a>

    </div>


    <!-- Beatrice Kosowski -->
