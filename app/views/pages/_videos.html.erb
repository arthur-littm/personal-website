<div class="container-fluid text-center section-header-white" id="videos" style="background-color: #00C3C7">
  <h1>Latest videos</h1>
  <div class="videos-container">
    <% @latest_videos.each do |id| %>
    <div class="video-card">
      <iframe width="340" height="210" src="https://www.youtube.com/embed/<%= id %>" frameborder="0" allowfullscreen></iframe>
    </div>
    <% end %>
  </div>
</div>
